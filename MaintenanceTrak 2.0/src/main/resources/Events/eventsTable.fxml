          <TableColumn fx:id="openedBy" prefWidth="100.0" text="Opened By" />
          <TableColumn fx:id="dateClosed" prefWidth="81.0" text="Date Closed" />
          <TableColumn fx:id="closedBy" prefWidth="100.0" text="Closed By" />
      </columns>
    </TableView>
</VBox>
